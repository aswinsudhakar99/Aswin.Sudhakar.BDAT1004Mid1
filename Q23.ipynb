   "source": [
    "def f(space, count):\n",
    "    if count > 0:\n",
    "        f(space, count // 2)\n",
    "        print(\" \" * space + \"*\" * count)\n",
    "        f(space + count // 2, count // 2)\n",
    "\n",
    "print(\">>> f(0, 0)\")\n",
    "f(0, 0)\n",
    "\n",
    "print(\"\\n>>> f(0, 1)\")\n",
    "f(0, 1)\n",
    "\n",
